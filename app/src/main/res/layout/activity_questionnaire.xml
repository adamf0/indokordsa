<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="action"
            type="com.app.indokordsa.view.ui.QuestionnaireActivity" />
        <variable
            name="kuesioner"
            type="com.app.indokordsa.view.model.KuesionerResult" />
        <variable
            name="topik"
            type="com.app.indokordsa.view.model.KuesionerResultDetail1" />
        <variable
            name="pertanyaan"
            type="com.app.indokordsa.view.model.KuesionerResultDetail2" />
        <import type="android.text.TextUtils"/>
        <import type="android.view.View"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/LayoutQuestionnaire"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.ui.QuestionnaireActivity">

        <include
            android:id="@+id/loader"
            layout="@layout/loading_page"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/LayoutInputQuestionnaire"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout7"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@color/background"
                app:layout_constraintBottom_toTopOf="@+id/guideline32"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView30"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:fontFamily="@font/helvetica_neue_condensed_bold"
                    android:gravity="end"
                    android:text="@{topik.no}"
                    android:textColor="@color/bg_header_title_questionnaire"
                    android:textSize="36sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/helvetica_neue_condensed_bold"
                    android:text="questionnaire"
                    android:textColor="@color/bg_header_title_questionnaire"
                    android:textSize="24sp"
                    app:layout_constraintEnd_toEndOf="@+id/textView30"
                    app:layout_constraintStart_toStartOf="@+id/textView30"
                    app:layout_constraintTop_toBottomOf="@+id/textView30" />

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="16dp"
                    android:fontFamily="@font/helvetica_neue_ultra_light"
                    android:text="@{kuesioner.area.nama}"
                    android:textColor="@color/bg_header_title_questionnaire"
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView31"
                    app:layout_constraintStart_toStartOf="@+id/textView31"
                    app:layout_constraintTop_toBottomOf="@+id/textView31" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.20109439" />

            <ScrollView
                android:fillViewport="true"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/constraintLayout8"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout7">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/textView33"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginLeft="16dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginRight="16dp"
                        android:fontFamily="@font/helvetica_neue_condensed_bold"
                        android:text="@{kuesioner.kuesioner.nama}"
                        android:textColor="#000"
                        android:textSize="34sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/txt_topik_LayoutInputChoiceQuestionnaire"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:fontFamily="@font/montserrat_aiternates_light"
                        android:text="@{kuesioner.jawaban.equals(`2`)? `(`+topik.tentang+`)`:``}"
                        android:textSize="24sp"
                        android:visibility="@{kuesioner.jawaban.equals(`2`)? View.VISIBLE:View.GONE}"
                        app:layout_constraintEnd_toEndOf="@+id/textView33"
                        app:layout_constraintStart_toStartOf="@+id/textView33"
                        app:layout_constraintTop_toBottomOf="@+id/textView33" />

                    <TextView
                        android:id="@+id/textView34"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_aiternates_light"
                        android:text="@{topik==null? ``:pertanyaan.pertanyaan}"
                        android:textColor="#000"
                        android:textSize="20sp"
                        app:layout_constraintEnd_toEndOf="@+id/textView33"
                        app:layout_constraintStart_toStartOf="@+id/textView33"
                        app:layout_constraintTop_toBottomOf="@+id/txt_topik_LayoutInputChoiceQuestionnaire" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/LayoutInputYNQuestionnaire"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="14dp"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="@+id/textView33"
                        app:layout_constraintStart_toStartOf="@+id/textView33"
                        app:layout_constraintTop_toBottomOf="@+id/textView34">

                        <Button
                            android:id="@+id/btn_ya_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="#000"
                            android:onClick="@{()->action.selectYN(`2`,kuesioner)}"
                            android:text="Ya"
                            android:textColor="#fff"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <Button
                            android:id="@+id/btn_tidak_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectYN(`1`,kuesioner)}"
                            android:text="Tidak"
                            android:textColor="#fff"
                            app:backgroundTint="@null"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_ya_questionnaire" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/LayoutInputChoiceQuestionnaire"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="14dp"
                        android:visibility="gone"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/textView33"
                        app:layout_constraintStart_toStartOf="@+id/textView33"
                        app:layout_constraintTop_toBottomOf="@+id/LayoutInputYNQuestionnaire">

                        <RadioGroup
                            android:id="@+id/rg_choice_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <RadioButton
                                android:id="@+id/btn_rbchoice1_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`Rusak`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="Rusak"
                                android:textColor="@drawable/radio_flat_text_selector" />

                            <RadioButton
                                android:id="@+id/btn_rbchoice2_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`Perlu modifikasi`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="Perlu modifikasi"
                                android:textColor="@drawable/radio_flat_text_selector" />

                            <RadioButton
                                android:id="@+id/btn_rbchoice3_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`Perlu ganti segera`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="Perlu ganti segera"
                                android:textColor="@drawable/radio_flat_text_selector" />

                            <RadioButton
                                android:id="@+id/btn_rbchoice4_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`Control mati`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="Control mati"
                                android:textColor="@drawable/radio_flat_text_selector" />

                            <RadioButton
                                android:id="@+id/btn_rbchoice5_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`CB trip`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="CB trip"
                                android:textColor="@drawable/radio_flat_text_selector" />

                            <RadioButton
                                android:id="@+id/btn_rbchoice6_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`Fault`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="Fault"
                                android:textColor="@drawable/radio_flat_text_selector" />

                            <RadioButton
                                android:id="@+id/btn_rbchoice7_questionnaire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:background="@drawable/radio_flat_selector"
                                android:button="@android:color/transparent"
                                android:gravity="center"
                                android:onClick="@{()->action.selectChoice(`Perlu di kalibrasi`,pertanyaan)}"
                                android:padding="10dp"
                                android:text="Perlu di kalibrasi"
                                android:textColor="@drawable/radio_flat_text_selector" />

                        </RadioGroup>

                        <Button
                            android:id="@+id/btn_choice1_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`Rusak`,pertanyaan)}"
                            android:text="Rusak"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <Button
                            android:id="@+id/btn_choice2_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`Perlu modifikasi`,pertanyaan)}"
                            android:text="Perlu modifikasi"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice1_questionnaire" />

                        <Button
                            android:id="@+id/btn_choice3_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`Perlu ganti segera`,pertanyaan)}"
                            android:text="Perlu ganti segera"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice2_questionnaire" />

                        <Button
                            android:id="@+id/btn_choice4_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`Control mati`,pertanyaan)}"
                            android:text="Control mati"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice3_questionnaire" />

                        <Button
                            android:id="@+id/btn_choice5_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`CB trip`,pertanyaan)}"
                            android:text="CB trip"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice4_questionnaire" />

                        <Button
                            android:id="@+id/btn_choice6_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`Fault`,pertanyaan)}"
                            android:text="Fault"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice5_questionnaire" />

                        <Button
                            android:id="@+id/btn_choice7_questionnaire"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="#000"
                            android:onClick="@{()->action.selectChoice(`Perlu di kalibrasi`,pertanyaan)}"
                            android:text="Perlu di kalibrasi"
                            android:visibility="visible"
                            app:backgroundTint="@null"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice6_questionnaire" />

                        <TextView
                            android:id="@+id/textView35"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            android:text="Start"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/btn_choice7_questionnaire" />

                        <EditText
                            android:id="@+id/editTextTextPersonName"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="HH:mm"
                            android:inputType="textPersonName"
                            android:text="@={pertanyaan.start}"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView35" />

                        <TextView
                            android:id="@+id/textView36"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="End"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

                        <EditText
                            android:id="@+id/editTextTextPersonName2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:ems="10"
                            android:hint="HH:mm"
                            android:inputType="textPersonName"
                            android:text="@={pertanyaan.end}"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView36" />

                        <TextView
                            android:id="@+id/textView37"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Duration"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

                        <EditText
                            android:id="@+id/editTextTextPersonName3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="8dp"
                            android:ems="10"
                            android:inputType="textPersonName"
                            android:text="@={pertanyaan.duration}"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView37" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </ScrollView>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout8"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_questionnaire"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:listitem="@layout/item_row_checklist_job" />

                <Button
                    android:id="@+id/btn_save_questionnaire"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginBottom="16dp"
                    android:background="@drawable/button_blue"
                    android:onClick="@{()->action.save()}"
                    android:paddingStart="13dp"
                    android:paddingLeft="13dp"
                    android:paddingTop="11dp"
                    android:paddingEnd="15dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="7dp"
                    android:text="Save"
                    android:visibility="gone"
                    app:backgroundTint="@null"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/btn_nextTopic_questionnaire"
                    app:layout_constraintStart_toEndOf="@+id/btn_backTopic_questionnaire"
                    app:layout_constraintTop_toBottomOf="@+id/rv_questionnaire" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/btn_backTopic_questionnaire"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:background="#000"
                    android:clickable="true"
                    android:focusable="true"
                    android:onClick="@{()->action.backTopic()}"
                    android:visibility="gone"
                    app:backgroundTint="#000"
                    app:fabSize="mini"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_save_questionnaire"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_save_questionnaire"
                    app:maxImageSize="18dp"
                    app:srcCompat="@drawable/ic_arrow_back"
                    app:tint="@android:color/white"
                    tools:ignore="VectorDrawableCompat" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/btn_nextTopic_questionnaire"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:background="#000"
                    android:clickable="true"
                    android:focusable="true"
                    android:onClick="@{()->action.nextTopic()}"
                    android:visibility="gone"
                    app:backgroundTint="#000"
                    app:fabSize="mini"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_save_questionnaire"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_save_questionnaire"
                    app:maxImageSize="18dp"
                    app:srcCompat="@drawable/ic_arrow_forward_24"
                    app:tint="@android:color/white"
                    tools:ignore="VectorDrawableCompat" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>